/* pages/activities/activities.wxss */
.button-icon {
  width: 36px;
  height: 36px;
  margin-left: 12px;
  margin-top: 16px;
  margin-bottom: 12px;
}
.toflex{
  display: flex; 
  justify-content: center;
  margin: 6px; 
}
.textinput {
  margin: 8px;
  height: 100px;
  width: 260px;
  border: 1px solid #ccc;
  border-radius: 10px;
  padding: 8px;
}
.round-click{
  height: 120rpx;
  width: 120rpx;
  background-color: #39C5BB;
  border-radius: 100%;
  position: fixed;
  bottom: 150rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.round-click navigator{
  font-size: 60rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}